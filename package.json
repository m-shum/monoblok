{
  "name": "monoblok",
  "private": true,
  "version": "0.1.0",
  "description": "Official monorepo for Storyblok open source packages",
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "build:tools": "cd tools/monoblok && pnpm install && pnpm build",
    "monoblok": "node tools/monoblok/dist/index.mjs",
    "check:versions": "pnpm monoblok check-versions",
    "fix:versions": "pnpm monoblok check-versions --fix"
  },
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "devDependencies": {
    "nx": "21.0.3",
    "typescript": "^5.7.2"
  },
  "resolutions": {
    "cypress": "14.3.3"
  },
  "dependencies": {
    "cypress": "^14.3.3"
  }
}
